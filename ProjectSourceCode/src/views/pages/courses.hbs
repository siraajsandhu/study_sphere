<style>
table {
  border-collapse: collapse;
  width: 100%;
}

th, td {
  padding: 8px;
  text-align: left;
  border-bottom: 1px solid #494949;
}

tr:hover {background-color: #88359c;}
</style>

{{> message}}

<table method="get" action= "/courses">

<thead>
    <tr>
    <th>Question</th>
    </tr>
</thead>
<tbody>
{{#each question}}
    <tr>
        <td> <a href="./unansweredQs.hbs?question_id={{question_id}}">{{question_name}}</a>  </td>
    </tr>
{{/each}}
</tbody>
</table>